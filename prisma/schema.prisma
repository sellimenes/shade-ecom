generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider      = "mysql"
  url           = env("DATABASE_URL")
  relationMode  = "prisma"
}

model User {
  id                String   @id @default(uuid())
  email             String   @unique
  hashedPassword    String?
  name              String?
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt
}